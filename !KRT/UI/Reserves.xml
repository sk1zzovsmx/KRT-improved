<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Frame name="KRTReserveListFrame" inherits="KRTFrameTemplate" toplevel="true" hidden="true" frameStrata="MEDIUM">
		<Size>
			<AbsDimension x="368" y="520" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Frames>
			<!-- ScrollFrame con ScrollChild -->
			<ScrollFrame name="$parentScrollFrame" inherits="KRTScrollFrameTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="16" y="-32" />
						</Offset>
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset>
							<AbsDimension x="-32" y="50" />
						</Offset>
					</Anchor>
				</Anchors>
				<ScrollChild>
					<Frame name="KRTReserveListFrameScrollChild">
						<Size>
							<AbsDimension x="1" y="1" />
						</Size>
					</Frame>
				</ScrollChild>
			</ScrollFrame>
			<Button name="$parentClearButton" inherits="KRTButtonTemplate">
				<Size>
					<AbsDimension x="100" y="24" />
				</Size>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT">
						<Offset>
							<AbsDimension x="16" y="20" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
			<Button name="$parentQueryButton" inherits="KRTButtonTemplate">
				<Size>
					<AbsDimension x="120" y="24" />
				</Size>
				<Anchors>
					<Anchor point="BOTTOM" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="0" y="20" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
			<Button name="$parentCloseButton" inherits="KRTButtonTemplate">
				<Size>
					<AbsDimension x="80" y="24" />
				</Size>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
						<Offset>
							<AbsDimension x="-16" y="20" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				KRT.Reserves:OnLoad(self)
			</OnLoad>
		</Scripts>
	</Frame>
	<!-- ====== End Reserve List Window ====== -->
</Ui>

