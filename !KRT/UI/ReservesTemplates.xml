<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
<!-- ====== Reserve List Templates ====== -->
<Button name="KRTReserveHeaderTemplate" virtual="true">
		<Size>
			<AbsDimension x="320" y="28" />
		</Size>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentLabel" inherits="GameFontNormal">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
					<Font font="Fonts\\FRIZQT__.TTF" height="14" flags="OUTLINE" />
					<Color r="1" g="0.82" b="0" a="1" />
				</FontString>
				<Texture name="$parentLeftLine" file="Interface\\Buttons\\WHITE8x8">
					<Size>
						<AbsDimension y="1" />
					</Size>
					<Anchors>
						<Anchor point="RIGHT" relativeTo="$parentLabel" relativePoint="LEFT">
							<Offset>
								<AbsDimension x="-6" y="0" />
							</Offset>
						</Anchor>
						<Anchor point="LEFT">
							<Offset>
								<AbsDimension x="4" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="1" g="1" b="1" a="0.3" />
				</Texture>
				<Texture name="$parentRightLine" file="Interface\\Buttons\\WHITE8x8">
					<Size>
						<AbsDimension y="1" />
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentLabel" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="6" y="0" />
							</Offset>
						</Anchor>
						<Anchor point="RIGHT">
							<Offset>
								<AbsDimension x="-4" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="1" g="1" b="1" a="0.3" />
				</Texture>
			</Layer>
		</Layers>
	</Button>
	<Frame name="KRTReserveRowTemplate" virtual="true">
		<Size>
			<AbsDimension x="320" y="42" />
		</Size>
		<Frames>
			<Button name="$parentIconBtn" inherits="KRTItemButtonTemplate">
				<Size>
					<AbsDimension x="32" y="32" />
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT">
						<Offset>
							<AbsDimension x="0" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
			<Frame name="$parentTextBlock">
				<Size>
					<AbsDimension x="0" y="32" />
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentIconBtn" relativePoint="RIGHT">
						<Offset>
							<AbsDimension x="8" y="0" />
						</Offset>
					</Anchor>
					<Anchor point="RIGHT" relativeTo="$parent" relativePoint="RIGHT">
						<Offset>
							<AbsDimension x="-6" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<FontString name="$parentName" inherits="KRTFontString">
							<Size>
								<AbsDimension x="0" y="0" />
							</Size>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset>
										<AbsDimension x="0" y="-3" />
									</Offset>
								</Anchor>
								<Anchor point="TOPRIGHT">
									<Offset>
										<AbsDimension x="0" y="-3" />
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
						<FontString name="$parentSource" inherits="GameFontHighlightSmall">
							<Size>
								<AbsDimension x="0" y="0" />
							</Size>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset>
										<AbsDimension x="0" y="-16" />
									</Offset>
								</Anchor>
								<Anchor point="RIGHT">
									<Offset>
										<AbsDimension x="0" y="0" />
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
						<FontString name="$parentPlayers" inherits="GameFontHighlightSmall" justifyH="LEFT">
							<Anchors>
								<Anchor point="TOPLEFT" relativeTo="$parentName" relativePoint="BOTTOMLEFT">
									<Offset>
										<AbsDimension x="0" y="-2" />
									</Offset>
								</Anchor>
								<Anchor point="TOPRIGHT" relativeTo="$parentName" relativePoint="BOTTOMRIGHT">
									<Offset>
										<AbsDimension x="0" y="-2" />
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBackground" file="Interface\\Buttons\\WHITE8x8">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="$parentQuantity" inherits="GameFontNormalSmall">
					<Anchors>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentIconBtn" relativePoint="BOTTOMRIGHT">
							<Offset>
								<AbsDimension x="-2" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
					<Font font="Fonts\\FRIZQT__.TTF" height="10" flags="OUTLINE" />
					<Color r="1" g="1" b="1" a="1" />
				</FontString>
			</Layer>
		</Layers>
	</Frame>
<!-- ====== End Reserve List Templates ====== -->
</Ui>
